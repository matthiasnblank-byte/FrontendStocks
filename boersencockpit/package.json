{
  "name": "boersencockpit",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:e2e": "ng serve --host 0.0.0.0",
    "build": "ng build",
    "lint": "node ./scripts/eslint-check-diff.mjs",
    "lint:all": "eslint . --ext .ts,.html",
    "format": "prettier --write \"**/*.{ts,html,css,md,json}\"",
    "format:check": "node ./scripts/prettier-check-diff.mjs",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "ci": "npm run lint && npm run test:cov && npm run build",
    "audit": "npm audit --audit-level=moderate || true",
    "licenses:check": "node ./scripts/check-licenses.mjs",
    "release": "semantic-release",
    "docker:build": "docker build -t boersencockpit:latest .",
    "docker:run": "docker run -p 8080:80 boersencockpit:latest",
    "e2e": "start-server-and-test start:e2e http://127.0.0.1:4200 cypress:run",
    "cypress:run": "cypress run",
    "e2e:open": "cypress open",
    "prepare": "husky install"
  },
  "engines": {
    "node": ">=20",
    "npm": ">=10"
  },
  "dependencies": {
    "@angular/animations": "^17.3.0",
    "@angular/cdk": "^17.3.10",
    "@angular/common": "^17.3.0",
    "@angular/compiler": "^17.3.0",
    "@angular/core": "^17.3.0",
    "@angular/forms": "^17.3.0",
    "@angular/platform-browser": "^17.3.0",
    "@angular/platform-browser-dynamic": "^17.3.0",
    "@angular/router": "^17.3.0",
    "@ngrx/effects": "^17.2.0",
    "@ngrx/entity": "^17.2.0",
    "@ngrx/router-store": "^17.2.0",
    "@ngrx/store": "^17.2.0",
    "@ngrx/store-devtools": "^17.2.0",
    "chart.js": "^4.5.0",
    "chartjs-plugin-annotation": "^3.1.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zod": "^3.23.8",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.17",
    "@angular-eslint/eslint-plugin": "^17.4.1",
    "@angular-eslint/eslint-plugin-template": "^17.4.1",
    "@angular-eslint/template-parser": "^17.4.1",
    "@angular/cli": "^17.3.17",
    "@angular/compiler-cli": "^17.3.0",
    "@commitlint/cli": "^20.0.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@ngrx/schematics": "^17.2.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.6",
    "@semantic-release/npm": "^12.0.2",
    "@semantic-release/release-notes-generator": "^14.1.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.11",
    "@typescript-eslint/eslint-plugin": "^7.17.0",
    "@typescript-eslint/parser": "^7.17.0",
    "autoprefixer": "^10.4.19",
    "cypress": "^13.13.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "http-server": "^14.1.1",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-marbles": "^3.1.1",
    "jest-preset-angular": "^14.4.2",
    "lint-staged": "^16.2.1",
    "postcss": "^8.4.41",
    "prettier": "^3.3.3",
    "semantic-release": "^24.2.9",
    "start-server-and-test": "^2.1.2",
    "tailwindcss": "^3.4.13",
    "ts-jest": "^29.2.5",
    "typescript": "~5.4.2"
  },
  "lint-staged": {
    "**/*.{ts,html}": [
      "eslint --fix",
      "prettier --write"
    ],
    "**/*.{css,json,md}": [
      "prettier --write"
    ]
  }
}
